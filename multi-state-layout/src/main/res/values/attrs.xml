<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiStateLayout">
        <attr name="msl_loadingLayout" format="reference" />
        <attr name="msl_emptyLayout" format="reference" />
        <attr name="msl_errorLayout" format="reference" />
        <attr name="msl_state" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
        </attr>
    </declare-styleable>

</resources>